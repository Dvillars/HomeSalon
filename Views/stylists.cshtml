@{
  Layout = "Shared/_Layout.cshtml";
}

<h1>| Stylists |</h1>
<form action="/stylists" method="post">
  <label for="stylist">Add New Stylists!!</label>
  <input id="stylist" type="text" name="stylist">

  <button class="btn" type="submit" name="button">Add!</button>
</form>
<ul>
  @foreach (var stylist in Model["Stylists"])
  {
  <li><a href="/stylists/@stylist.GetId()">@stylist.GetName()</a></li>
  }
</ul>
<span>| <a href="/">Home</a> |</span>
<span>| <a href="/clients">View Clients</a> |</span>
